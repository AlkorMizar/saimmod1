            <StackPanel>
                <Grid Canvas.Top="396" Width="200" Canvas.Left="19" HorizontalAlignment="Left" Height="97" VerticalAlignment="Center">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="29*"/>
                        <ColumnDefinition Width="121*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="R0" HorizontalAlignment="Right"/>
                    <Label Content="a" Grid.Row="1" HorizontalAlignment="Right"/>
                    <Label Content="m" Grid.Row="2" HorizontalAlignment="Right"/>

                    <TextBox x:Name="R0Text" Text="15" Grid.Column="1" Margin="3"/>
                    <TextBox x:Name="aText" Text="1643" Grid.Column="1" Grid.Row="1" Margin="3"/>
                    <TextBox x:Name="mText" Text="12031278" Grid.Column="1" Grid.Row="2" Margin="3"/>
                </Grid>
                <Button Content="Calculate" Margin="60 10 0 0" Width="150" HorizontalAlignment="Left" Click="OnCalculate"/>
            </StackPanel>
            <Grid Margin="25 0 0 0">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <RadioButton Grid.Column="0" Grid.Row="0" GroupName="Distributions" Checked="CheckUniform" Content="Равномерное"/>
                <RadioButton Grid.Column="0" Grid.Row="1" GroupName="Distributions" Checked="CheckExponential" Content="Экспоненциальное "/>
                <RadioButton Grid.Column="0" Grid.Row="2" GroupName="Distributions" Checked="CheckedTriangle1" Content="Треунгольное(x-a)"/>
                <RadioButton Grid.Column="0" Grid.Row="3" GroupName="Distributions" Checked="CheckedTriangle2" Content="Треунгольное(b-x)"/>

                <RadioButton Grid.Column="1" Grid.Row="0" GroupName="Distributions" Checked="CheckedGamma" Content="Гамма"/>
                <RadioButton Grid.Column="1" Grid.Row="1" GroupName="Distributions" Checked="CheckedGauss" Content="Гауссовское"/>
                <RadioButton Grid.Column="1" Grid.Row="2" GroupName="Distributions" Checked="CheckedSimpson" Content="Симпсона"/>
            </Grid>

            <Grid Margin="25 0 0 0" Width="450" Height="80">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="29*"/>
                    <ColumnDefinition Width="121*"/>
                    <ColumnDefinition Width="29*"/>
                    <ColumnDefinition Width="121*"/>
                    <ColumnDefinition Width="29*"/>
                    <ColumnDefinition Width="121*"/>
                </Grid.ColumnDefinitions>
