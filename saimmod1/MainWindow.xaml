                <Label Grid.Row="1" Grid.Column="0" Content="Дисперсия, D =" HorizontalAlignment="Right"/>
                <TextBlock Grid.Row="1" Grid.Column="0" Width="90"  TextWrapping="Wrap" HorizontalAlignment="Left" Margin="31,39,0,0" Grid.RowSpan="2">
                    Среднее квадратическое 
                    отклонение, σ =
                </TextBlock>
                <Label x:Name="mathExpect" Grid.Row="0" Grid.Column="1" Content="0"/>
                <Label x:Name="dispersion" Grid.Row="1" Grid.Column="1" Content="0"/>
                <Label x:Name="deviation" Grid.Row="2" Content="0" VerticalAlignment="Top" Grid.ColumnSpan="2" Height="24" Margin="121,19,0,0"/>
            </Grid>
            
