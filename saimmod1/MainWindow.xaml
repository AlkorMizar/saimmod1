                <Label Content="b=" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Right"/>
                <Label Content="m=" Grid.Row="0" Grid.Column="2" HorizontalAlignment="Right"/>
                <Label Content="σ=" Grid.Row="1" Grid.Column="2" HorizontalAlignment="Right"/>
                <Label Content="λ=" Grid.Row="0" Grid.Column="4" HorizontalAlignment="Right"/>
                <Label Content="η=" Grid.Row="1" Grid.Column="4" HorizontalAlignment="Right"/>

                <TextBox x:Name="a"  Grid.Column="1" Grid.Row="0" Margin="3"/>
                <TextBox x:Name="b" Grid.Column="1" Grid.Row="1" Margin="3"/>
                <TextBox x:Name="m" Grid.Column="3" Grid.Row="0" Margin="3"/>
                <TextBox x:Name="sigm" Grid.Column="3" Grid.Row="1" Margin="3"/>
                <TextBox x:Name="lambda" Grid.Column="5" Grid.Row="0" Margin="3"/>
                <TextBox x:Name="n" Grid.Column="5" Grid.Row="1" Margin="3"/>
            </Grid>

            <Grid Margin="25 0 0 0" >
